{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "@database-url",
      "SECRET_KEY": "@secret-key"
    },
    "functions": {
      "api/**.js": {
        "maxDuration": 10
      }
    }
  }
  